{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-clean-code"],
    "jsRules": {},
    "rules": {
        "no-commented-out-code": false,
        "no-console": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "object-literal-sort-keys": false
    },
    "linterOptions": {
        "exclude": ["src/**/*.spec.ts", "src/**/*.test.ts"]
    },
    "rulesDirectory": []
}
